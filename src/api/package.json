{
  "name": "car-maintenance-api",
  "version": "1.0.0",
  "description": "Car Maintenance API - LoopBack 4 - M3",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
